{
  "name": "typescript-project-template",
  "version": "1.0.0",
  "description": "A template for new typescript projects",
  "main": "./dist/Main.js",
  "scripts": {
    "start": "node ./dist/Main.js",
    "watch": "tsc -w",
    "build": "tsc -p tsconfig.json",
    "clean-build": "rimraf tsconfig.tsbuildinfo dist/ && tsc -p tsconfig.json",
    "create-change-log": "node ./scripts/create-change-log.js",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "yarn@3.4.1",
  "devDependencies": {
    "@types/node": "^18.14.1",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "keep-a-changelog": "^2.2.1",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@typegoose/typegoose": "^10.2.0",
    "logform": "^2.5.1",
    "mongoose": "^7.0.0",
    "winston": "^3.8.2"
  }
}
